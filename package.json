{
  "name": "webpage_resume",
  "version": "1.0.0",
  "description": "Portfolio page",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon ./lib/server/index.js --exec babel-node",
    "server": "babel lib --out-dir build --copy-files",
    "dev": "npm run clear && export NODE_ENV='development' && webpack --config ./webpack/webpack.dev.config.js && npm run dev:server",
    "prod": "npm run clear && export NODE_ENV='production' && npm run server && webpack -p --config ./webpack/webpack.prod.config.js && node ./build/server/index.js",
    "clear": "rm -rf ./build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Express",
    "React",
    "Sass"
  ],
